{
  "name": "proyecto-sytw",
  "version": "1.0.0",
  "description": "proyecto de la asignatura",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "server": "tsc-watch --onSuccess \"env-cmd -f ./config/dev.env node dist/index.js\"",
    "mongoose": "tsc-watch --onSuccess \"env-cmd -f ./config/dev.env dist/db/mongoose.js\"",
    "tsx": "tsc --outDir dist",
    "doc": "typedoc",
    "build": "tsc",
    "start": "node dist/index.js",
    "startTest": "env-cmd -f ./config/dev.env node dist/index.js",
    "jest": "jest --verbose",
    "test": "env-cmd -f ./config/dev.env mocha --exit",
    "coverage": "c8 npm test && c8 report --reporter=lcov"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "loader": [
      "ts-node/register"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.4",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lowdb": "^1.0.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.9.0",
    "@types/prompt-sync": "^4.2.0",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.11.6",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "inquirer": "^7.3.3",
    "jest-cucumber": "^3.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.2.0",
    "color": "^4.2.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^1.0.0",
    "mongoose": "^7.6.5",
    "nodemon": "^3.0.1",
    "prompt-sync": "^4.2.0",
    "request": "^2.88.2",
    "validator": "^13.9.0",
    "yargs": "^17.7.2"
  }
}
